<div id="picture_card" class="card">
</div>
<div id="card_background" class="card">
  <div id="list" class="p-3 mx-auto">
    <h1 id="header">All lists</h1>
    <%= link_to 'Create a new list', new_list_path, id: "create_button" %>
    <ul>
      <% @lists.each do |list| %>
        <li><%= link_to list.name, list_path(list.id) %></li>
      <% end %>
    </ul>
  </div>
</div>
